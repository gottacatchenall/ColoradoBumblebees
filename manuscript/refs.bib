  langid = {english},
  keywords = {co-occurrence,networks,spatial ecology},
  file = {/Users/michael/Zotero/storage/B5LJ4PUL/Gravel et al. - 2019 - Bringing Elton and Grinnell together a quantitati.pdf;/Users/michael/Zotero/storage/JXBYF83S/ecog.html}
