@article{Ren2023ForEdg,
  title = {Forest Edges Increase Pollinator Network Robustness to Extinction with Declining Area},
  author = {Ren, Peng and Didham, Raphael K. and Murphy, Mark V. and Zeng, Di and Si, Xingfeng and Ding, Ping},
  year = 2023,
