@article{Barthell2025BumbleBee,
  title = {Bumble Bee Niche Overlap along an Elevation Gradient: How Traits Can Inform Novel Competitive Pressures under Climate Change},
  shorttitle = {Bumble Bee Niche Overlap along an Elevation Gradient},
