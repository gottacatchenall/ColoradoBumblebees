  langid = {english},
  keywords = {Balearic islands,flower density,interaction rewiring,modularity,multilayer centrality,network dissimilarity,plant abundance,species turnover},
  file = {/Users/michael/Zotero/storage/5MVA3N3S/Hervías-Parejo et al. - Spatio-temporal variation in plant–pollinator inte.pdf}
}

@article{Higgins1999PrePla,
